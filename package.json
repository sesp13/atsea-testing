{
  "name": "final-homework-tests",
  "version": "1.0.0",
  "description": "Final homework tests for Praxis 2021-2",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint test/**/*.{ts,js} src/**/*.{ts,js}",
    "build": "npm run clean && tsc",
    "test": "mocha -r tsconfig-paths/register -r ts-node/register \"test/**/*.test.ts\"",
    "test:api": "npm run build && mocha -t 5000 --reporter mochawesome --recursive ./dist/test/api/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sesp13/atsea-testing.git"
  },
  "keywords": [
    "test"
  ],
