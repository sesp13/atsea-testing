  "bugs": {
    "url": "https://github.com/sesp13/atsea-testing/issues"
  },
  "homepage": "https://github.com/sesp13/atsea-testing#readme",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/superagent": "^4.1.13",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "http-status-codes": "^2.1.4",
    "mocha": "^9.1.3",
    "mochawesome": "^7.0.1",
    "superagent": "^6.1.0",
